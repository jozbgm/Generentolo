# Gemini API Key (optional - users can provide their own)
VITE_API_KEY=your_gemini_api_key_here

# ClipDrop Upscaling API (optional - for image upscaling feature)
VITE_CLIPDROP_API_KEY=your_clipdrop_api_key_here
VITE_ENABLE_UPSCALING=true

# Google Custom Search API (optional - for Google Search Grounding feature)
# Get your API key: https://developers.google.com/custom-search/v1/overview
# Create search engine: https://programmablesearchengine.google.com/
VITE_GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
VITE_GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here
